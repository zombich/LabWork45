<Window x:Class="LabWork45.Task2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LabWork45"
        mc:Ignorable="d"
        Title="Task2" Height="450" Width="800">
    <StackPanel>
        <Label>Команда MSSQL</Label>
        <TextBox x:Name="MSSQLCommandTextBox"/>
        <Button Content="Отправить команду" Click="SendMSSQLCommandButton_Click"/>
        <Label>Команда SQLite</Label>
        <TextBox x:Name="SQLiteCommandTextBox"/>
        <Button Content="Отправить команду" Click="SendSQLiteCommandButton_Click"/>
    </StackPanel>
</Window>
